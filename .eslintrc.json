{
  "extends": "standard",
  "parser": "babel-eslint",
  "plugins": ["angular"],
  "rules": {
    "arrow-parens": ["error", "as-needed"],
    "comma-dangle": ["error", "never"],
    "eol-last": ["error", "always"],
    "max-len": ["error", 100],
    "object-curly-spacing": ["error", "always"],
    "object-property-newline": "error",
    "no-unused-vars": "error",
    "prefer-const": "error",
    "quotes": ["error", "single"]
  }
}
